<%- include('../partials/header'); %>
<h1>Our Products</h1>

<div class="product-list">
  <% products.forEach(product => { %>
  <div class="product">
    <img
      src="<%= product.image %>"
      alt="<%= product.name %>"
    />
    <h3><%= product.name %></h3>
    <p>Price: $<%= product.price %></p>
  </div>
  <% }) %>
</div>
<%- include('../partials/footer'); %>
